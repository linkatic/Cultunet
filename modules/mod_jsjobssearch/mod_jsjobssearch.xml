<?xml version="1.0" encoding="utf-8"?>
<install type="module" version="1.5">
	<name>Search JS Jobs</name>
	<author>Joom Sky</author>
	<creationDate>Nov 14th, 2009</creationDate>
	<copyright>(C) 2009. All rights reserved.</copyright>
	<license>GNU/GPL</license>
	<authorEmail>ahmad@joomsky.com</authorEmail>
	<authorUrl>www.joomsky.com</authorUrl>
	<version>1.2</version>
	<description>Job Search form for JS Jobs. </description>
	<params>
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="moduleclass_sfx" type="text" default="" label="Module Class Suffix" description="PARAMMODULECLASSSUFFIX" />
		<param name="@spacer" type="spacer" default="" label="" description="" />
		<param name="category" type="radio" default="1" label="Category" description="Show/Hide the category">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="jobtype" type="radio" default="1" label="Job Type" description="Show/Hide the job type">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="areaprofesional" type="radio" default="1" label="Area profesional" description="Show/Hide the profesional area">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="areagestion" type="radio" default="1" label="Area de gestion" description="Show/Hide the gestion area">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="jobstatus" type="radio" default="1" label="Job Status" description="Show/Hide the job status">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="salaryrange" type="radio" default="1" label="Salary Range" description="Show/Hide the salary range">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="heighesteducation" type="radio" default="1" label="Heighest Education" description="Show/Hide the heighest education">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="shift" type="radio" default="1" label="Shift" description="Show/Hide the job shift">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="experience" type="radio" default="1" label="Experience" description="Show/Hide the experience">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="durration" type="radio" default="1" label="Duration" description="Show/Hide the duration">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="startpublishing" type="radio" default="1" label="Start Publishing Date" description="Show/Hide the start publishing date">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="stoppublishing" type="radio" default="1" label="Stop Publishing Date" description="Show/Hide the stop publishing date">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="company" type="radio" default="1" label="Company" description="Show/Hide the company">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
		<param name="addresses" type="radio" default="1" label="Addresses" description="Show/Hide the addresses">
			<option value="0">Hide</option>
			<option value="1">Show</option>
		</param>
	</params>
	<files>
	<filename module="mod_jsjobssearch">mod_jsjobssearch.php</filename>
	</files>
	<languages>
		<language tag="en-GB">en-GB.mod_jsjobssearch.ini</language>
	</languages>
</install>
